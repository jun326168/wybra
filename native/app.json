{
  "expo": {
    "name": "Wybra",
    "slug": "wybra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wybra",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jun326168.wybra",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.988306602712-u2sok3anlj4s2h02jgvame73bos2186b"
            ]
          }
        ],
        "NSPhotoLibraryUsageDescription": "Wybra 需要存取你的相簿，以便你上傳頭像或其它相片。"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#121418",
        "foregroundImage": "./assets/images/icon.png",
        "backgroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121418"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#121418",
          "sounds": []
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Wybra 需要存取你的相簿，以便你上傳頭像或其它相片。"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "96b456a5-a3b7-4bf4-95c0-f23d679d6a0d"
      }
    }
  }
}
